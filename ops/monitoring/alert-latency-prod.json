{
  "displayName": "apex-prod-api latency p95 high",
  "combiner": "OR",
  "conditions": [
    {
      "displayName": "Cloud Run request latency p95 > 500ms",
      "conditionThreshold": {
        "filter": "resource.type=\"cloud_run_revision\" AND resource.label.\"service_name\"=\"apex-prod-api\" AND metric.type=\"run.googleapis.com/request_latencies\"",
        "aggregations": [
          {
            "alignmentPeriod": "300s",
            "perSeriesAligner": "ALIGN_PERCENTILE_95",
            "crossSeriesReducer": "REDUCE_MEAN"
          }
        ],
        "comparison": "COMPARISON_GT",
        "thresholdValue": 500,
        "duration": "300s",
        "trigger": { "count": 1 }
      }
    }
  ],
  "enabled": true,
  "notificationChannels": [],
  "documentation": {
    "content": "Check Cloud Run concurrency, DB latency, and recent deploy changes.",
    "mimeType": "text/markdown"
  }
}
